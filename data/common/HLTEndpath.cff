ilterPixelTrackIsolatedTauJetsMuonTau, hltElectronTrackIsolFilterElectronTau, hltElectronTrackIsolFilterHOneOEMinusOneOPFilterElectronTau, hltFilterIsolatedTauJetsL3ElectronTau, hltFilterPixelTrackIsolatedTauJetsElectronTau, hltBSoftmuonL3filter, MuBIsoL3IsoFiltered, hltBLifetimeL3filter, MuBIsoL3IsoFiltered, hltBLifetimeL3filter, ElBElectronTrackIsolFilter, hltemuL1IsoSingleElectronTrackIsolFilter, hltemuNonIsoL1IsoTrackIsolFilter, EMuL3MuonIsoFilter, hltNonIsoEMuL3MuonPreFilter, MuJetsL3IsoFiltered, MuJetsHLT1jet40, MuNoL2IsoJetsL3IsoFiltered, MuNoL2IsoJetsHLT1jet40, MuNoIsoJetsL3PreFiltered, MuNoIsoJetsHLT1jet50, isolPixelTrackFilter}
        }
//
// Put all these modules into a common endpath
endpath HLTEndpath1 = {l1gtTrigReport & hltTrigReport}


//
// This to be put as a final path at the end of HLTpaths.cff:
//
// Construct new HLT-data-model summary objects
//module triggerSummaryAOD = triggerSummaryProducerAOD from "HLTrigger/HLTcore/data/triggerSummaryProducerAOD.cfi"
module triggerSummaryRAW = triggerSummaryProducerRAW from "HLTrigger/HLTcore/data/triggerSummaryProducerRAW.cfi"
//
// (RAW producer only for a subset of events!)
module triggerSummaryRAWprescaler = hltPrescaler from "HLTrigger/HLTcore/data/hltPrescaler.cfi"
//
// Final path not to accept any event
module  boolFinal = hltBool from "HLTrigger/HLTfilters/data/hltBool.cfi"
replace boolFinal.result=false
//
sequence triggerFinalPath = { 
    triggerSummaryAOD &
    triggerSummaryRAWprescaler & 
    triggerSummaryRAW & 
    boolFinal
}

